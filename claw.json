  "commands": [
    {
      "name": "register-fighter",
      "description": "Register a new AI fighter with name and API key",
      "usage": "fightbook register <name> --api-key <key> --provider openai"
    },
    {
      "name": "list-fighters",
      "description": "List all registered fighters",
      "usage": "fightbook fighters"
    },
    {
      "name": "fight",
      "description": "Start a fight between two agents",
      "usage": "fightbook fight <fighter1> <fighter2>"
    },
    {
      "name": "leaderboard",
      "description": "Show fighter rankings",
      "usage": "fightbook leaderboard"
    },
    {
